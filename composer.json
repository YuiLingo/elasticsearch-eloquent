{
    "name": "isswp101/elasticsearch-eloquent",
    "type": "library",
    "description": "Eloquent-like models to Elasticsearch with advanced features",
    "keywords": [
        "isswp101",
        "elasticsearch-eloquent"
    ],
    "homepage": "https://github.com/isswp101/elasticsearch-eloquent",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Sorokin",
            "email": "isswp101@gmail.com",
            "homepage": "https://github.com/isswp101",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Isswp101\\Persimmon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Isswp101\\Persimmon\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
